            <kind>1</kind>
            <string>var Date = string(current_day) + "/" + string(current_month) + "/" +  string(current_year);
var Time = string(current_hour) + "_" + string(current_minute) + "_" + string(current_second);

var TotalTime = string(global.SpdTotalTimeMin) + ":" + string(global.SpdTotalTimeSec);

var EasyTimeA = string(global.SpdTimeMinEasy) + ":" + string(global.SpdTimeSecEasy);
var EasyTimeB = string(global.SpdTimeMinEasy) + ":" + "0" + string(global.SpdTimeSecEasy);

var MediumTimeA = string(global.SpdTimeMinMedium) + ":" + string(global.SpdTimeSecMedium);
var MediumTimeB = string(global.SpdTimeMinMedium) + ":" + "0" + string(global.SpdTimeSecMedium);

var HardTimeA = string(global.SpdTimeMinHard) + ":" + "0" + string(global.SpdTimeSecHard);
var HardTimeB = string(global.SpdTimeMinHard) + ":" + string(global.SpdTimeSecHard);


if (global.SpdTimeSecEasy &lt; 10)
{   EasyTime = EasyTimeB    }
else
{   EasyTime = EasyTimeA   }

if (global.SpdTimeSecMedium &lt; 10)
{   MediumTime = MediumTimeB    }
else
{   MediumTime = MediumTimeA    }

if (global.SpdTimeSecHard &lt; 10)
{   HardTime = HardTimeA    }
else
{   HardTime = HardTimeB    }

File = file_text_open_write(global.SpdDir + "\Speedrun" + string(global.SpeedrunNum) + ".txt");
global.SpeedrunNum += 1;

file_text_write_string(File, "BLACK BOX JUMPER");
file_text_writeln(File);
file_text_write_string(File, "Speedrun statistics");
file_text_writeln(File);
file_text_write_string(File, "===============");
file_text_writeln(File);
file_text_writeln(File);

file_text_write_string(File, "Game versions: " + string(global.GameVersion));
file_text_writeln(File);
file_text_write_string(File, "Speedrun number: " + string(global.SpeedrunNum - 1));
file_text_writeln(File);
file_text_write_string(File, "Date: " + string(Date));
file_text_writeln(File);
file_text_write_string(File, "Time: " + string(Time));
file_text_writeln(File);
file_text_write_string(File, "---------------");
file_text_writeln(File);
file_text_write_string(File, "Attempts: " + string(global.Attempts));
file_text_writeln(File);
file_text_write_string(File, "Jumps: " + string(global.Jumps));
file_text_writeln(File);
file_text_write_string(File, "Score: 0");
file_text_writeln(File);
file_text_writeln(File);
file_text_write_string(File, "Total time: " + string(TotalTime));
file_text_writeln(File);
file_text_writeln(File);
file_text_write_string(File, "Easy: " + string(EasyTime));
file_text_writeln(File);
file_text_write_string(File, "Medium: " + string(MediumTime));
file_text_writeln(File);
file_text_write_string(File, "Hard: " + string(HardTime));
file_text_writeln(File);

if global.Cheats = 1
{
    file_text_writeln(File);
    file_text_write_string(File, "**************");
    file_text_writeln(File);
    file_text_write_string(File, "CHEATS WERE ENEALED!");
    file_text_writeln(File);
    file_text_write_string(File, "**************");
    file_text_writeln(File);
}

file_text_close(File);
</string>
          </argument>
