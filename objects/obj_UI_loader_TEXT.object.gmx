if file_exists(working_directory + "\Game data" + "\GameInfo.dat")
{
    ini_open(working_directory + "\Game data" + "\GameInfo.dat");
    global.Attempts = ini_read_real("GameInfo","Attempts",0);
    global.TutorialFinished = ini_read_real("GameInfo","TutorialFinished",0);
    global.SpeedrunNum = ini_read_real("GameInfo","Speedrun Number",1);
    global.Session = ini_read_real("GameInfo","Session",0);
    
